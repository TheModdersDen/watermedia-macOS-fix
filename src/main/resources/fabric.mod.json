{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",
  "name": "${name}",
  "description": "${moddesc}",
  "authors": ${modauthorsarray},
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/${id}",
    "sources": "https://github.com/${github_project}",
    "issues": "https://github.com/${github_project}/issues",
  },

  "license": "${modlicense}",
  "icon": "pack.png",
  "environment": "client",
  "entrypoints": {
    "preLaunch": [
      "me.srrapero720.watermedia.loaders.FavricLoader"
    ]
  },
  "breaks": {
    "fancyvideo_api": "*",
  },
  "depends": {
    "java": ">=8"
  }
}